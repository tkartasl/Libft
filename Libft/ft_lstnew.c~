#include "libft.h"
#include <stdio.h>
#include <stdlib.h>

t_list	*ft_lstnew(void *content)
{
	t_list	*new;

	new = NULL;
	new = (t_list *)malloc(sizeof(t_list));
	if (new == 0)
		return (0);
	new->content = content;
	new->next = NULL;
	return (new);
}

int main()
{
	char 	s[]= "Moroo";
	t_list	*res;

	res = ft_lstnew(s);
	printf("%s", (char *)res->content);
	printf("%p", res->next);
}
