#include "libft.h"
#include <stdio.h>

void	ft_test(void *p)
{
	(*(int *)p) = (*(int *)p) + 1;
}

void	ft_lstiter(t_list *lst, void (*f)(void *))
{
	t_list	*current;

	current = lst;
	while (current != 0)
	{
		f(current->content);
		current = current->next;
	}
}

int main()
{
	void (*f)(void *);
	t_list  *head;
    t_list  *one = NULL;
    t_list  *two = NULL;
    t_list *extra = NULL;
	int i = 1;
	int j = 2;
	int k = 3;
	int h = 4;
	int *p = &i;
	int *o = &j;
	int *m = &k;
	int *c = &h;

    f = &ft_test;
	head = malloc(sizeof(t_list));
    one = malloc(sizeof(t_list));
    two = malloc(sizeof(t_list));
    extra = malloc(sizeof(t_list));
    extra->content = c;
    one->content = o;
    two->content = m;
    head->content = p;
    head->next = one;
	one->next = two;
    two->next = extra;
    extra->next = NULL;

	ft_lstiter(head, f);
	while (head != 0)
	{
		printf("%d\n", *(int *)head->content);
		head = head->next;
	}
	return (0);	
}
