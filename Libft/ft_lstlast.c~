#include "libft.h"
#include <stdio.h>

t_list	*ft_lstlast(t_list *lst)
{
	t_list	*current;
	
	current = lst;
	while (current->next != 0)
	{
		current = current->next;
	}
	return (current);
}

int main()
{
	t_list	*head;
	t_list	*one;
	t_list  *two;
	t_list  *three;
	t_list  *four;
	t_list	*result = NULL;

	head = ft_lstnew("1");
	one = ft_lstnew("2");
	two = ft_lstnew("3");
	three = ft_lstnew("4");
	four = ft_lstnew(":)");
	
	head->next = one;
	one->next = two;
	two->next = three;
	three->next = four;
	four->next = NULL;
	
	result = ft_lstlast(head);
	printf("%s", result->content);
	return (0);
}
